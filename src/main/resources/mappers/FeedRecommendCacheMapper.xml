<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.ht.project.snsproject.mapper.FeedRecommendCacheMapper">

    <select id="getFeedInfoByLatestOrder" resultType="int">
        SELECT id
        FROM feeds
        WHERE publicScope = 'ALL'
        GROUP BY id
        ORDER BY id DESC
        limit 10;
    </select>
</mapper>